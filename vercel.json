{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "env": {
      "DB_USER": "@db_user",
      "DB_PASSWORD": "@db_password",
      "DB_HOST": "@db_host",
      "DB_PORT": "@db_port",
      "DB_DATABASE": "@db_database",
      "DB_SSL": "@db_ssl"
    }
  }
  